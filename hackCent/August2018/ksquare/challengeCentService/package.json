{
  "name": "challengeCent",
  "version": "1.0.0",
  "description": "challengeCent package.json from the documentation, supplemented with testing support",
  "scripts": {
    "start:win": "start-mongod.bat && start-server.bat && npm run dev:win",
    "stop:win": "stop-mongod.bat && stop-server.bat",
    "dev:win": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "deploy:win": "deploy.bat",
    "start:nix": "./start-mongod.sh & ./start-server.sh & npm run dev:nix",
    "stop:nix": "./stop-mongod.sh & ./stop-server.sh",
    "dev:nix": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "deploy:nix": "./deploy.sh",
    "clean:nix": "./clean.sh",
    "pree2e": "npm run build:e2e",
    "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
    "preprotractor": "webdriver-manager update",
    "protractor": "protractor protractor.config.js",
    "pretest": "npm run build",
    "test": "start mongod && ping 127.0.0.1 -n 6 > null && start node server.js && concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
    "pretest:once": "npm run build",
    "test:once": "karma start karma.conf.js --single-run",
    "lint": "tslint ./src/**/*.ts -t verbose --fix",
    "ng": "ng",
    "clean": "rm -rf dist",
    "clean:win": "del /f /q dist",
    "copy": "cp -r dist/. /Users/jagadeesh.pala/Desktop/Temp/dist",
    "copy:win": "xcopy  /s /y /e /q dist\\. E:\\Apache24\\htdocs\\dist\\",
    "build": "npm i && npm run clean && ng build && npm run copy",
    "build:win": "npm i && npm run clean:win && ng build && npm run copy:win",
    "start_dev": "ng serve -p 3000 --host 0.0.0.0",
    "build_prod": "npm i && npm run clean && ng build --prod"
  },
  "keywords": [],
  "author": "Jags",
  "license": "MIT",
